{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "completed-docs": [
      false,
      {
        "classes": true,
        "types": true
      }
    ],
    "max-classes-per-file": false,
    "no-unused-variable": true,
    "semicolon": false
  },
  "rulesDirectory": []
}
